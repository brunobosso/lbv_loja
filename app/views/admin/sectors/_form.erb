<%= form_for [:admin, sector], html: { class: 'form-horizontal' } do |f| %>
	<div class="form-group">
	  <%= f.label :name %>:
	  <%= f.text_field :name,class: 'form-control' %>
	</div>
	<div class="row">
	    <div class="col-md-3 col-md-offset-5">
		  <%= f.submit "Salvar",class: "btn btn-sm btn-success" %>
		  <%= link_to "Deletar", admin_sectors_path(sector) , data: { method: :delete},class: 'btn btn-sm btn-danger' %>
	  	  <%= link_to "Voltar", :back, class: "btn btn-sm btn-default" %>
	  </div>
	</div>
<% end %>